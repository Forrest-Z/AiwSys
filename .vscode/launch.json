    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug Go  file",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}"
        },
        {
            "name": "Debug Python",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": [],
            "console": "integratedTerminal"
        },
        {
            "name": "Debug Test",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bazel-bin/modules/test/test_torch/test_torch",
            "args": ["addressbook.data",],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Debug Cyber",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bazel-bin/cyber/examples/listener",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "CYBER_PATH",
                    "value": "${workspaceFolder}/cyber",
                },
                {
                    "name": "GLOG_log_dir",
                    "value": "${workspaceFolder}/logs"
                },
                {
                    "name": "CYBER_DOMAIN_ID",
                    "value": "80"
                },
                {
                    "name": "CYBER_IP",
                    //"value":"192.168.0.227"
                    "value": "127.0.0.1"
                },
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Debug Monitor",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bazel-bin/cyber/mainboard",
            "args": [
                "-d",
                "${workspaceFolder}/bazel-AiwSys/modules/monitor/dag/monitor.dag",
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "CYBER_PATH",
                    "value": "${workspaceFolder}/cyber",
                },
                {
                    "name": "GLOG_log_dir",
                    "value": "${workspaceFolder}/logs"
                },
                {
                    "name": "CYBER_DOMAIN_ID",
                    "value": "80"
                },
                {
                    "name": "CYBER_IP",
                    //"value":"192.168.0.227"
                    "value": "127.0.0.1"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            // "preLaunchTask": "Bazel Build Auto",
            "miDebuggerPath": "/usr/bin/gdb"
        },
        {
            "name": "Debug Projects",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bazel-bin/cyber/mainboard",
            "args": [
                "-d",
                "${workspaceFolder}/projects/conductor_rail/production/dag/wear.dag",
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "CYBER_PATH",
                    "value": "${workspaceFolder}/cyber",
                },
                {
                    "name": "GLOG_log_dir",
                    "value": "${workspaceFolder}/logs"
                },
                {
                    "name": "CYBER_DOMAIN_ID",
                    "value": "80"
                },
                {
                    "name": "CYBER_IP",
                    //"value":"192.168.0.227"
                    "value": "127.0.0.1"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            // "preLaunchTask": "Bazel Build Auto",
            "miDebuggerPath": "/usr/bin/gdb"
        },
    ]
}